server {
    listen 80;
    server_name 159.65.90.145;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/jesterhead/portfolio-project;
    }
    location /media/ {
        root /home/jesterhead/portfolio-project;
    }


    location / {
        include proxy_params;
        proxy_pass http://unix:/home/jesterhead/portfolio-project/portfolio.sock;
    }
}